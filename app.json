{
  "expo": {
    "name": "quiick-ride-rider",
    "slug": "quiick-ride-rider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quiickriderider",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.quiickride.rider",
      "entitlements": {
        "aps-environment": "production"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCEgN-LLuqFBE7nDzqa2zdgE-iYq-bKhQE"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCEgN-LLuqFBE7nDzqa2zdgE-iYq-bKhQE"
        }
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.quiickride.rider",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "autolink": true
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "extraPods": [
              {
                "name": "FirebaseCore",
                "modular_headers": true
              },
              {
                "name": "FirebaseCoreInternal",
                "modular_headers": true
              },
              {
                "name": "GoogleUtilities",
                "modular_headers": true
              }
            ]
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9f69d1e5-9c29-479e-8665-64a23b5e8982"
      }
    },
    "owner": "abdulgumi",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4fbdeb48-29da-4590-9698-b71daff7a19a"
    }
  }
}