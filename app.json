{
  "expo": {
    "name": "KEKE GO",
    "slug": "kekego",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kekego",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.kekego.rider",
      "entitlements": {
        "aps-environment": "production"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCEgN-LLuqFBE7nDzqa2zdgE-iYq-bKhQE",
        "jsEngine": "hermes",
        "useNonExemptEncryption": false
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "Keke Go needs your location to find nearby drivers and track your ride.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Keke Go needs your location to provide accurate pickup estimates and ride tracking.",
        "NSCameraUsageDescription": "Keke Go needs camera access to allow you to update your profile picture.",
        "NSPhotoLibraryUsageDescription": "Keke Go needs photo library access to allow you to choose a profile picture.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCEgN-LLuqFBE7nDzqa2zdgE-iYq-bKhQE"
        },
        "jsEngine": "hermes"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.kekego.rider",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "autolink": true
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "buildReactNativeFromSource": true,
            "extraPods": [
              {
                "name": "FirebaseCore",
                "modular_headers": true
              },
              {
                "name": "FirebaseCoreInternal",
                "modular_headers": true
              },
              {
                "name": "GoogleUtilities",
                "modular_headers": true
              },
              {
                "name": "react-native-maps",
                "modular_headers": true,
                "path": "../node_modules/react-native-maps"
              },
              {
                "name": "RNFBApp",
                "modular_headers": true,
                "path": "../node_modules/@react-native-firebase/app"
              },
              {
                "name": "Google-Maps-iOS-Utils",
                "modular_headers": true
              }
            ]
          },
          "android": {
            "lintOptions": {
              "checkReleaseBuilds": false,
              "abortOnError": false
            }
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "abdullahgumi",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/21841566-bb5f-4557-abe0-cd308f4b2acf"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "21841566-bb5f-4557-abe0-cd308f4b2acf"
      }
    }
  }
}